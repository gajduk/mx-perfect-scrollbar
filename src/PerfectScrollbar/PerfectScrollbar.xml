<?xml version="1.0" encoding="utf-8" ?>
<widget id="PerfectScrollbar.widget.PerfectScrollbar" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>PerfectScrollbar</name>
    <description>The description of this widget.</description>

    <icon>   </icon>

    <properties>
        <property key="scrollTopAttribute" type="attribute" required="false" defaultValue="" allowNonPersistableEntities="true" >
            <caption>Scroll Top Attribute</caption>
            <category>DataSource</category>
            <description>An integer attribute that initilizes scrolling in y-axis to a specific offset.</description>
            <attributeTypes>
                <attributeType name="Integer"/>
            </attributeTypes>
        </property>
        <property key="scrollLeftAttribute" type="attribute" required="false" defaultValue="" allowNonPersistableEntities="true" >
            <caption>Scroll Left Attribute</caption>
            <category>DataSource</category>
            <description>An integer attribute that initilizes scrolling in x-axis to a specific offset.</description>
            <attributeTypes>
                <attributeType name="Integer"/>
            </attributeTypes>
        </property>
        <property key="customOptions" type="string" required="false" defaultValue="{}" multiline="true">
            <caption>Custom Options</caption>
            <category>Beaviour</category>
            <description>For a list of available options check - https://github.com/utatti/perfect-scrollbar#options</description>
        </property>
    </properties>
</widget>
